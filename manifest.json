{
  "manifest_version":2,

  "name":"Websets",
  "description": "Save a set of webpages to be opened at any time of need. :)",
  "version": "1.0",

  "browser_action":{
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png"
   },

  "permissions":[
    "tabs",
    "storage"
  ]
}
